{
  "name": "proclame",
  "version": "1.0.0",
  "description": "Voici le projet Proclame, un projet de d√©monstration pour l'examen.",
  "scripts": {
    "dev:client": "pnpm --filter client dev",
    "dev:server": "pnpm --filter server dev",
    "dev": "concurrently \"pnpm dev:client\" \"pnpm dev:server\"",
    "build:client": "pnpm --filter client build",
    "build:server": "pnpm --filter server build",
    "build": "pnpm build:client && pnpm build:server",
    "generate": "drizzle-kit generate --config=packages/server/src/config/drizzle.ts",
    "migrate": "tsx packages/server/src/config/migrate.ts"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "concurrently": "^8.2.2",
    "csurf": "^1.11.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "three": "^0.170.0",
    "zustand": "^5.0.8"
  },
  "author": "Evan Azemard",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/csurf": "^1.11.5",
    "cross-env": "^7.0.3",
    "vite-plugin-svgr": "^4.5.0"
  }
}
